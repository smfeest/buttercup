{
  "extends": "tslint:recommended",
  "rules": {
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "no-unused-expression": [
      true,
      "allow-new"
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ]
  }
}
